{
  "name": "tier2-chatbot",
  "version": "1.0.0",
  "description": "Advanced AI-powered chatbot system with lead capture, analytics, and multi-industry support",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "echo 'No build step required for Node.js application'",
    "clean": "rm -rf logs/*.log",
    "setup": "npm install && mkdir -p logs data",
    "demo": "node server.js & sleep 3 && open http://localhost:3001/demo/law-firm"
  },
  "keywords": [
    "chatbot",
    "ai",
    "lead-capture",
    "customer-service",
    "automation",
    "openai",
    "legal",
    "real-estate",
    "ecommerce"
  ],
  "author": "BuildAIFor.Me",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "morgan": "^1.10.0",
    "dotenv": "^16.3.1",
    "openai": "^4.20.1",
    "uuid": "^9.0.1",
    "moment": "^2.29.4",
    "validator": "^13.11.0",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.6",
    "nodemailer": "^6.9.7",
