{
  "buildCommand": "cd website && npm install && npm run build",
  "outputDirectory": "website/dist",
  "installCommand": "cd website && npm install",
  "framework": "vite",
  "env": {
    "NODE_ENV": "production",
    "ROLLUP_SKIP_NATIVE": "true",
    "ESBUILD_BINARY_PATH": ""
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
