{
  "name": "rag-ai-tool-frontend",
  "version": "1.0.0",
  "description": "Professional RAG AI Tool - Enterprise Document Intelligence Platform",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "setup": "npm install && npm run build",
    "start": "npm run dev"
  },
  "keywords": [
    "rag",
    "ai",
    "document-search",
    "semantic-search",
    "knowledge-management",
    "enterprise",
    "supabase",
    "openai",
    "react",
    "typescript"
  ],
  "author": "RAG AI Tool Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/rag-ai-tool-client.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/rag-ai-tool-client/issues"
  },
  "homepage": "https://github.com/your-org/rag-ai-tool-client#readme",
